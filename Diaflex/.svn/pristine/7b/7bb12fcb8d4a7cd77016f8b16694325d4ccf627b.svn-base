package ft.com.dao;

public class TrmsDao {
    int    relId;
    String trmDtl;
    String typeDtl;

    public TrmsDao() {
        super();
    }

    public void setRelId(int relId) {
        this.relId = relId;
    }

    public int getRelId() {
        return relId;
    }

    public void setTrmDtl(String trmDtl) {
        this.trmDtl = trmDtl;
    }

    public String getTrmDtl() {
        return trmDtl;
    }

    public void setTypeDtl(String typeDtl) {
        this.typeDtl = typeDtl;
    }

    public String getTypeDtl() {
        return typeDtl;
    }
}


