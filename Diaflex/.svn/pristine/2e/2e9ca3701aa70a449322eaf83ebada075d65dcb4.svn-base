package ft.com.contact;

//~--- non-JDK imports --------------------------------------------------------

import org.apache.struts.action.ActionForm;
import org.apache.struts.action.DynaActionForm;

//~--- JDK imports ------------------------------------------------------------

import java.util.HashMap;
import java.util.Map;

public class SearchContactFrm extends ActionForm {
    public final Map values = new HashMap();
    private String   nme, nmeFltr, addr, attrDtl,usr;

    public SearchContactFrm() {
        super();
    }

    public void setValue(String key, Object value) {
        values.put(key, value);
    }

    public Object getValue(String key) {
        return values.get(key);
    }

    public Map getValues() {
        return values;
    }

    public void setNme(String nme) {
        this.nme = nme;
    }

    public String getNme() {
        return nme;
    }

    public void setAddr(String addr) {
        this.addr = addr;
    }

    public String getAddr() {
        return addr;
    }

    public void setAttrDtl(String attrDtl) {
        this.attrDtl = attrDtl;
    }

    public String getAttrDtl() {
        return attrDtl;
    }

    public void setNmeFltr(String nmeFltr) {
        this.nmeFltr = nmeFltr;
    }

    public String getNmeFltr() {
        return nmeFltr;
    }

    public void setUsr(String usr) {
        this.usr = usr;
    }

    public String getUsr() {
        return usr;
    }
}


