package ft.com.dao;

public class DFMenuItm {
    int    srt, dfMenuIdn;
    String typ, dsp, img, lnk, dsc, prj_typ , flg,rmk;

    public DFMenuItm() {
        super();
    }

    public void setTyp(String typ) {
        this.typ = typ;
    }

    public String getTyp() {
        return typ;
    }

    public void setDsp(String dsp) {
        this.dsp = dsp;
    }

    public String getDsp() {
        return dsp;
    }

    public void setImg(String img) {
        this.img = img;
    }

    public String getImg() {
        return img;
    }

    public void setLnk(String lnk) {
        this.lnk = lnk;
    }

    public String getLnk() {
        return lnk;
    }

    public void setDsc(String dsc) {
        this.dsc = dsc;
    }

    public String getDsc() {
        return dsc;
    }

    public void setPrj_typ(String prj_typ) {
        this.prj_typ = prj_typ;
    }

    public String getPrj_typ() {
        return prj_typ;
    }

    public void setSrt(int srt) {
        this.srt = srt;
    }

    public int getSrt() {
        return srt;
    }

    public void setDfMenuIdn(int dfMenuIdn) {
        this.dfMenuIdn = dfMenuIdn;
    }

    public int getDfMenuIdn() {
        return dfMenuIdn;
    }

    public void setFlg(String flg) {
        this.flg = flg;
    }

    public String getFlg() {
        return flg;
    }

    public void setRmk(String rmk) {
        this.rmk = rmk;
    }

    public String getRmk() {
        return rmk;
    }
}


